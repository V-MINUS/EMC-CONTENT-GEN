{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_DISABLE_ESLINT": "1",
    "TYPESCRIPT_IGNORE_ERRORS": "1",
    "SKIP_TYPESCRIPT_CHECK": "1",
    "NEXT_TELEMETRY_DISABLED": "1",
    "CI": "false"
  }
}
